{
  "name": "@koralle/vitest-axe",
  "version": "1.0.0-pre.3",
  "description": "Custom Vitest matcher for testing accessibility with aXe. Forked from jest-axe.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koralle/vitest-axe.git"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903",
  "license": "MIT",
  "author": "koralle <mkhd7930@gmail.com>",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./matchers": {
      "types": "./dist/matchers.d.ts",
      "default": "./dist/matchers.js"
    },
    "./extend-expect": {
      "types": "./dist/extend-expect.d.ts",
      "default": "./dist/extend-expect.js"
    }
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "extend-expect.d.ts",
    "extend-expect.js",
    "matchers.d.ts",
    "matchers.js"
  ],
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write '**/*.{ts,js}'",
    "format-check": "prettier --check '**/*.{ts,js}'",
    "clean": "rm -rf dist && rm -rf node_modules"
  },
  "dependencies": {
    "axe-core": "^4.7.2",
    "chalk": "^5.3.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@types/aria-query": "^5.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "esbuild": "^0.23.0",
    "eslint": "^9.7.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "fast-glob": "^3.3.1",
    "globals": "^15.8.0",
    "jsdom": "^24.1.0",
    "lefthook": "^1.7.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "pretty-format": "^28.1.3",
    "tsup": "^7.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "vitest": "^0.34.3"
  },
  "peerDependencies": {
    "vitest": ">=0.31.0"
  }
}
